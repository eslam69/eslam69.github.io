<Layout title={"CV - " + myName}>
  <div class="max-w-3xl mx-auto">
    <section class="mb-12">
      <h1 class="text-4xl font-bold mb-4">{myName}</h1>
      <p class="text-xl text-gray-600 dark:text-gray-300 mb-4">
